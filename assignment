echo "<hr/>";
$arr1=array("w",3,4,5);
$arr2=array("w",10,4,5,44,77);
$thesame=array(0,0,0);
$s=0;
$diff=array(0,0,0,0);
$d=0;
for($i=0;$i<count($arr2);$i++){
for($j=0;$j<count($arr1);$i++){
if($arr1[$j]==$arr2[$i]){
$thesame[$s];
$s++;
}else{
$diff[$d];
$d++;
}
}
}
echo "The Same :";
for($i=0;$i<count($thesame);$i++){
echo $thesame."<br>";
}
echo "<hr>";
echo "Diff :";
for($i=0;$i<count($diff);$i++){
echo $diff."<br>";
}
