$num=array(2,8,6,66,10,999);
$max=0;
$sum=0;
for($i=0;$i<count($num);$i++){
if($num[$i]>$max){
$max=$num[$i]
}
$sum=$sum+$num[$i];
}
echo "Max: ".$max;
