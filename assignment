echo "8:";
echo "<hr/>";
echo "Sorted Num :";
for($i=0;$i<count($num);$i++){
$temp=0;
for($j=$i;$j<count($num);$j++){
if($num[$i]>$num[$j]){
$temp=$num[$j];
$num[$j]=$num[$i];
$num[$i]=$temp;
}
}
echo $num[$i];
}
//السؤال التاسع
